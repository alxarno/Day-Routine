{
    "extends": ["tslint:latest"],
    "rules": {
        "jsx-wrap-multiline": false,
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "ordered-imports":false,
        "no-implicit-dependencies": [true, ["src", "assets"]],
        "no-submodule-imports": [true, "src", "assets"]
    }
}